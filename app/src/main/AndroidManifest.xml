        </activity>
        <activity android:name=".UQ"
            android:label="@string/place_UQ"
            />
        <activity android:name=".southBankActivity"
            android:label="@string/place_South_bank"
            />
        <activity android:name=".mtLookoutActivity"
            android:label="@string/plact_MtCootTha_Lookout"/>
        <activity android:name=".galleryActivity"
